{
    "presets": ["@babel/preset-env", "@babel/preset-react"],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      ["react-css-modules", {
        "generateScopedName": "[name]__[local]--[hash:base64:5]",
        "filetypes": {
          ".less": {
            "syntax": "postcss-less"
          }
        },
        "autoResolveMultipleImports": true
      }],
      ["import", {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css"
      }]
    ]
  }